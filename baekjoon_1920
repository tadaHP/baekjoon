#include <iostream>
#include <string>
#include <algorithm>

using std::cout; using std::cin; using std::string;

int main() {
	std::ios_base::sync_with_stdio(0);
	cin.tie(0);
	int n;
	int m;
	int tmp;

	cin >> n;

	int *num = new int[n];

	for (int i = 0; i < n; i++) {
		cin >> num[i];
	}

	cin >> m;
	
	std::sort(num, num + n);

	for (int i = 0; i < m; i++) {
		cin >> tmp;
		cout << std::binary_search(num, num + n, tmp) << "\n";
	}
	return 0;
}
